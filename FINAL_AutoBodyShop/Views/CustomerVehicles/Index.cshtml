@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Vehicle_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Car_Model)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Year)
